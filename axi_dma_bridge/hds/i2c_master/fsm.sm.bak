second (MLText
uid 20,0
va (VaSet
)
xt "938,16474,26038,124474"
st "--- register ---
process(clk)
  variable bus_we_q: std_logic;
 variable reset_txflag_q: std_logic;
 variable reset_rxflag_q: std_logic;
begin
   if clk='1' and clk'event then
      if reset = '1' then
          reg00 <= (others => '0');
          reg02 <= (others => '0');
          reg03 <= (others => '0');
          txflag <= '0';
          rxflag <= '0';
          rtxflag <= '0';
          bus_we_q := '0';
      elsif bus_we = '1'   then -- only sensitive to rising edge of bus_we
          if cs(0) = '1'  then
               reg00 <= bus_din; -- data tx register
               txflag <= '1';
          elsif cs(2) = '1'  then
               reg02 <= bus_din; -- -- read msg length
               rxflag <= '1';
          elsif cs(3) = '1' then
               reg03 <= bus_din; -- dst i2C address
          elsif cs(4) = '1'  then
               reg00 <= bus_din; -- -- data tx with Sr
               rtxflag <= '1';
               txflag <= '1';
          elsif cs(5) = '1'  then
               reg02 <= bus_din; -- -- read msg length
               rxflag <= '1';
               rtxflag <= '1';
          end if;
       end if;
       bus_we_q := bus_we;
       if reset_txflag = '1' and reset_txflag_q = '0'  then
         txflag <= '0';
      end if;
       if reset_rxflag = '1' and reset_rxflag_q = '0'   then
         rxflag <= '0';
      end if;
       if reset_rtxflag = '1' then
         rtxflag <= '0';
      end if;
    reset_txflag_q := reset_txflag;
    reset_rxflag_q := reset_rxflag;
    end if;
end process;


process(bus_cs, bus_addr,reg00,reg01,reg02,reg03)
begin
   cs <= (others => '0');
   bus_dout <= (others => '0');
   if bus_cs = '1' then
      if bus_addr = 0 then
           cs(0) <= '1';   
           bus_dout <= reg00;
     elsif bus_addr = 1 then
           cs(1) <= '1';   
           bus_dout <= reg01;
     elsif bus_addr = 2 then
           cs(2) <= '1';   
           bus_dout <= reg02;
     elsif bus_addr = 3 then
           cs(3) <= '1';   
           bus_dout <= reg03;
     elsif bus_addr = 4 then
           cs(4) <= '1';   
           bus_dout <= reg01;
     elsif bus_addr = 5 then
           cs(5) <= '1';   
           bus_dout <= reg02;
     end if;
   end if;
end process;

process(clk)
begin
   if clk='1' and clk'event then
      if reset = '1' then
         en_cnt <= (others => '0');
     else
        if en = '1' then
            en_cnt <= (others => '0');
       else
           en_cnt <= en_cnt+1;
       end if;
     end if;
   end if;
end process;

