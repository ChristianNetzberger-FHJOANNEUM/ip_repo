second (MLText
uid 26,0
va (VaSet
)
xt "56,24768,36656,97768"
st "bus_addr <=sbus_i.addr;
bus_we <= sbus_i.we;
bus_rd <= sbus_i.rd;
bus_din <= sbus_i.wdata(15 downto 0);

process(clk)
begin
   if clk='1' and clk'event then  
      sbus_o.ack <= bus_ack;
      sbus_o.rdata <=  (others => '0');   
      if bus_cs = '1' and bus_rd = '1' then 
           sbus_o.rdata <= X\"0000\" & bus_dout ;
       end if;
     end if;
end process;

-- addr decoder
  bus_cs <= '1' when bus_addr >= addr_base_g and bus_addr < (addr_base_g + addr_range_c) else '0';
  bus_ack <= bus_cs and (bus_rd or bus_we);

--dual port memory------------------------------------
mem1_we <= bus_cs and bus_we;
bus_ptr <= conv_integer(bus_addr(4 downto 0));
--reg_ptr <= conv_integer(reg_addr_int(4 downto 0));

--memory bus->registers-----
process(clk)
begin
   if clk='1' and clk'event then  
      if mem1_we = '1'  then
         mem1(bus_ptr) <= bus_din;
       end if;
     end if;
end process;

--memory registers--> bus -----
process(clk)
begin
   if clk='1' and clk'event then  
      if mem2_we = '1'  then
         mem2(reg_wrptr) <= reg2bus_data;
       end if;
     end if;
end process;

reg2bus_data <= reg_tmp & i2creg_dout when reg_mux = '1' else
                              i2creg_dout & X\"00\";

bus_dout <= mem1(bus_ptr) when bus_addr(5) = '0' else mem2(bus_ptr)  ;
mem1_rdata <= mem1(reg_rdptr);

i2creg_addr <= reg_addr_int;
i2csel <= i2csel_int;
--reg_dout <= reg_dout_int;

---automatic scheduling of xfers ----------
process(clk)
begin
   if clk='1' and clk'event then
      if reset = '1' then
           auto_timer <= (others => '0');
           auto_en <= '0';
      else  
           auto_timer <= auto_timer+1;
           auto_en <= '0';
           if auto_timer = 10000000 then
     --     if auto_timer = 100000 then
                 auto_timer <= (others => '0');
                 auto_en <= '1';
            end if;
      end if;
   end if;
end process;"
tm "ArchConcStmtTextMgr"
)
associable 1
)
